{
    "name": "sllh/iso-codes-validator-bundle",
    "type": "symfony-bundle",
    "description": "Provides easy implementation of IsoCodesValidator",
    "license": "MIT",
    "authors": [
        {
            "name": "Sullivan Sénéchal",
            "email": "soullivaneuh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": "~2.3",
        "symfony/finder": "~2.3",
        "sllh/iso-codes-validator": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "satooshi/php-coveralls": "~0.6",
        "codeclimate/php-test-reporter": "dev-master",
        "matthiasnoback/symfony-dependency-injection-test": "~0.7"
    },
    "autoload": {
        "psr-4": { "SLLH\\IsoCodesValidatorBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "SLLH\\IsoCodesValidatorBundle\\Tests\\": "tests" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
